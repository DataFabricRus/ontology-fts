    fibo-fnd-aap-agt:isIdentifiedBy [
        a fibo-be-le-lei:LegalEntityIdentifier, fibo-be-le-fbo:OrganizationIdentifier, fibo-fnd-arr-id:Identifier ;
        fibo-fnd-aap-agt:identifies :LE-1 ;
        fibo-fnd-rel-rel:hasUniqueIdentifier "..."
    ] ;
    fibo-be-le-lei:hasAddressOfLegalFormation :Datafabric-Address ;
    .